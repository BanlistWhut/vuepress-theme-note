{
    "name": "vuepress-theme-note",
    "version": "1.0.1-alpha.0",
    "description": "vuepress theme ",
    "main": "skeleton-entry.js",
    "repository": "git@github.com:JackRay-C/vuepress-theme-note.git",
    "author": "renhj",
    "license": "MIT",
    "private": false,
    "scripts": {
        "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 1 && git add CHANGELOG.md",
        "release": "standard-version",
        "alpha": "standard-version -- --prerelease alpha && git push --follow-tags origin master && npm publish --tag alpha"
    },
    "husky": {
        "hooks": {
            "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
        }
    },
    "dependencies": {
        "vue": "^2.6.11",
        "vue-server-renderer": "^2.6.11",
        "vuepress": "^1.4.1",
        "vuex": "^3.1.3"
    },
    "devDependencies": {
        "@vuepress/plugin-active-header-links": "^1.4.1",
        "@vuepress/plugin-back-to-top": "^1.4.1",
        "@vuepress/plugin-medium-zoom": "^1.4.1",
        "@vuepress/plugin-nprogress": "^1.4.1",
        "@vuepress/plugin-search": "^1.4.1",
        "autoprefixer": "^9.7.6",
        "conventional-changelog": "^3.1.18",
        "conventional-changelog-cli": "^2.0.31",
        "cross-env": "^7.0.2",
        "cz-conventional-changelog": "^3.1.0",
        "cz-customizable": "^6.2.0",
        "html-webpack-plugin": "^4.2.0",
        "husky": "^4.2.5",
        "node-sass": "^4.13.1",
        "sass-loader": "^8.0.2",
        "standard-version": "^7.1.0",
        "style-loader": "^1.1.4",
        "vue-loader": "^15.9.1",
        "vue-template-compiler": "^2.6.11",
        "webpack": "^4.42.1",
        "webpack-cli": "^3.3.11",
        "webpack-dev-server": "^3.10.3"
    },
    "config": {
        "commitizen": {
            "path": "./node_modules/cz-conventional-changelog"
        }
    }
}